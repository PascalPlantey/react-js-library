{
  "name": "@pascalplantey/react-js-library",
  "version": "0.0.2",
  "description": "My personal React & JS Library",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "types/",
  "homepage": "https://github.com/PascalPlantey/react-js-library.git",
  "repositery": {
    "type": "git",
    "url": "git+https://github.com/PascalPlantey/react-js-library.git"
  },
  "publishConfig": {
    "tag": "latest"
  },
  "keywords": [
    "react",
    "react-hooks",
    "hooks",
    "utilities"
  ],
  "author": "Pascal Plantey <pascal.plantey@gmail.com>",
  "license": "ISC",
  "scripts": {
    "build-lib": "rollup -c",
    "build-lib:watch": "rollup -c --watch",
    "build-types": "tsc",
    "build-types:watch": "tsc --watch",
    "build-storybook": "storybook build",
    "build-docs": "jsdoc -c jsdoc.json --readme ./README.md",
    "build-all": "npm build-lib && npm build-types && npm build-docs && npm build-storybook",
    "storybook:start": "storybook dev -p 6006"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.22.7",
    "@babel/preset-env": "^7.22.7",
    "@babel/preset-react": "^7.22.5",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-dynamic-import-vars": "^2.0.4",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@storybook/addon-essentials": "^7.0.27",
    "@storybook/addon-interactions": "^7.0.27",
    "@storybook/addon-links": "^7.0.27",
    "@storybook/blocks": "^7.0.27",
    "@storybook/react": "^7.0.27",
    "@storybook/react-webpack5": "^7.0.27",
    "@storybook/testing-library": "^0.2.0",
    "classnames": "^2.3.2",
    "docdash": "^2.0.1",
    "jsdoc": "^4.0.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.79.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook": "^7.0.27",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "classnames": "^2.3.2",
